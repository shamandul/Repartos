<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context="es.jspsoluciones.repartos.EditarDatosZonasFragment"
    android:orientation="vertical"
    android:background="@color/material_grey_600">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/modificar_zona"
        android:id="@+id/textView4"
        style="@style/Base.TextAppearance.AppCompat.Title"
        android:textColor="#ffffff"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_margin="10dp" />
    <View android:layout_height="2dip" android:layout_width="wrap_content"
        android:background="@color/colorAceptar" />
    <ListView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:id="@+id/listaElementoZonaEditar" />


</LinearLayout>
